{
	"llvm-target":   "wasm32-unknown-unknown",
	"cpu":           "generic",
	"features":      "",
	"build-tags":    ["tinygo.wasm", "polkawasm", "runtime_memhash_leveldb"],
	"goos":          "linux",
	"goarch":        "arm",
	"gc": 					 "extalloc",
	"linker":        "wasm-ld",
	"libc":          "",
	"scheduler":     "none",
	"default-stack-size": 16384,
	"cflags": [],
	"ldflags": [
		"--no-entry",
		"--no-demangle",
		"--import-memory",
		"--allow-undefined",
		"--export=__heap_base",
  	"--export=__data_end",
		"--export-table"
	],
	"emulator":      "wasmtime {}",
	"wasm-abi":      "generic"
}