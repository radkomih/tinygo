{
	"llvm-target": "wasm32-unknown-unknown",
	"cpu": "generic",
	"features": "+bulk-memory,+nontrapping-fptoint,+sign-ext",
	"build-tags": [
		"tinygo.wasm",
		"polkawasm",
		"runtime_memhash_leveldb"
	],
	"goos": "linux",
	"goarch": "arm",
	"gc": "conservative",
	"linker": "wasm-ld",
	"libc": "wasi-libc",
	"scheduler": "none",
	"default-stack-size": 16384,
	"cflags": [
		"-mbulk-memory",
		"-mnontrapping-fptoint",
		"-msign-ext"
	],
	"ldflags": [
		"--initial-memory=1310720",
		"--no-demangle",
		"--stack-first",
		"--import-memory",
		"--allow-undefined",
		"--export=__heap_base",
		"--export=__data_end",
		"--export-table"
	],
	"extra-files": [
		"src/runtime/asm_tinygowasm.S"
	],
	"emulator": "wasmtime {}",
	"wasm-abi": "generic"
}